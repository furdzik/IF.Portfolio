{
  "name": "if.portfolio",
  "version": "1.4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furdzik/if.portfolio.git"
  },
  "scripts": {
    "start": "npm run lang && npm run dev",
    "build": "cross-env NODE_ENV=production npx webpack --progress --env production",
    "build:prod": "node ./scripts/create-env.js && npm run build",
    "lint": "npm run eslint && npm run stylelint",
    "lang": "extract-messages -l=en -d=en -o src/lang/generated --flat \"src/**/**/**/*.messages.js\"",
    "find-duplicates": "jscpd ./src/",
    "stylelint": "npx stylelint \"**/*.styles.{js,jsx}\"",
    "eslint": "eslint src/ --ext .jsx,.js",
    "dev": "npx webpack-dev-server --history-api-fallback --open --env development",
    "outdated": "npm outdated",
    "audit": "npm audit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.2.1",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@formatjs/intl-pluralrules": "5.2.6",
    "@formatjs/intl-relativetimeformat": "11.2.6",
    "@material-ui/core": "4.12.4",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@mdi/js": "7.2.96",
    "@mdi/react": "1.6.1",
    "@sentry/react": "8.33.1",
    "@sentry/tracing": "7.70.0",
    "axios": "0.28.0",
    "caniuse-lite": "1.0.30001538",
    "core-js": "3.32.2",
    "cross-env": "7.0.3",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.7",
    "react-redux": "8.1.2",
    "react-router-dom": "6.16.0",
    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "reselect": "4.1.8",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.22.15",
    "@babel/core": "7.22.20",
    "@babel/eslint-parser": "7.22.15",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-runtime": "7.22.15",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-react": "7.22.15",
    "@babel/runtime": "7.22.15",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@emotion/babel-plugin": "11.11.0",
    "@emotion/babel-preset-css-prop": "11.11.0",
    "babel-loader": "9.1.3",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-react-intl-auto": "3.3.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "commitlint": "17.7.1",
    "commitlint-config-kamisama": "0.3.3",
    "copy-webpack-plugin": "11.0.0",
    "dotenv": "16.3.1",
    "editorconfig": "1.0.4",
    "eslint": "8.50.0",
    "eslint-config-kamisama": "0.3.3",
    "eslint-import-resolver-babel-module": "5.3.2",
    "extract-react-intl-messages": "4.1.1",
    "file-loader": "6.2.0",
    "full-icu": "1.5.0",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "jscpd": "3.5.10",
    "postcss": "8.4.31",
    "postcss-styled-syntax": "0.5.0",
    "react-hot-loader": "4.13.1",
    "react-test-renderer": "18.2.0",
    "redux-devtools-extension": "2.13.9",
    "stylelint": "15.10.3",
    "stylelint-config-kamisama": "0.4.7",
    "webpack": "5.88.2",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-cdn-plugin": "3.3.1",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0"
  }
}
