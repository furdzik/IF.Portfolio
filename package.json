{
  "name": "if.portfolio",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furdzik/if.portfolio.git"
  },
  "scripts": {
    "start": "npm run lang && npm run dev",
    "build": "cross-env NODE_ENV=production npx webpack --progress --env production",
    "build:prod": "node ./scripts/create-env.js && npm run build",
    "lint": "npm run eslint && npm run stylelint",
    "lang": "extract-messages -l=en -d=en -o src/lang/generated --flat \"src/**/**/**/*.messages.js\"",
    "find-duplicates": "jscpd ./src/",
    "stylelint": "npx stylelint \"**/*.styles.{js,jsx}\"",
    "eslint": "eslint src/ --ext .jsx,.js",
    "dev": "npx webpack-dev-server --history-api-fallback --open --env development"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.1.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@formatjs/intl-pluralrules": "4.3.0",
    "@formatjs/intl-relativetimeformat": "9.5.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@mdi/js": "6.5.95",
    "@mdi/react": "1.5.0",
    "@sentry/react": "7.0.0",
    "@sentry/tracing": "7.0.0",
    "axios": "0.25.0",
    "caniuse-lite": "1.0.30001301",
    "core-js": "3.20.3",
    "cross-env": "7.0.3",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.3",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "reselect": "4.1.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.16.8",
    "@babel/core": "7.16.12",
    "@babel/eslint-parser": "7.16.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-transform-runtime": "7.16.10",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/runtime": "7.16.7",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@emotion/babel-plugin": "11.7.2",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@furdzik/commitlint-config": "0.0.4",
    "@furdzik/eslint-config": "0.0.7",
    "@furdzik/stylelint-config": "0.0.4",
    "babel-loader": "8.2.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-react-intl-auto": "3.3.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-styled-components": "2.0.2",
    "commitlint": "17.0.2",
    "commitlint-config-kamisama": "0.3.0",
    "copy-webpack-plugin": "10.2.1",
    "dotenv": "14.2.0",
    "editorconfig": "0.15.3",
    "eslint": "8.7.0",
    "eslint-config-kamisama": "0.3.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "extract-react-intl-messages": "4.1.1",
    "file-loader": "6.2.0",
    "full-icu": "1.4.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "jscpd": "3.4.5",
    "postcss": "8.4.5",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "17.0.2",
    "redux-devtools-extension": "2.13.9",
    "stylelint": "14.2.0",
    "stylelint-config-kamisama": "0.3.0",
    "stylelint-processor-styled-components": "1.10.0",
    "webpack": "5.73.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cdn-plugin": "3.3.1",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-cli": "4.9.2",
