{
  "name": "if.portfolio",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furdzik/if.portfolio.git"
  },
  "scripts": {
    "start": "npm run lang && npm run dev",
    "build": "cross-env NODE_ENV=production npx webpack --progress --env production",
    "build:prod": "node ./scripts/create-env.js && npm run build",
    "lint": "npm run eslint && npm run stylelint",
    "lang": "extract-messages -l=en -d=en -o src/lang/generated --flat \"src/**/**/**/*.messages.js\"",
    "find-duplicates": "jscpd ./src/",
    "stylelint": "npx stylelint \"**/*.styles.{js,jsx}\"",
    "eslint": "eslint src/ --ext .jsx,.js",
    "dev": "npx webpack-dev-server --history-api-fallback --open --env development"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.1.2",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@formatjs/intl-pluralrules": "4.3.2",
    "@formatjs/intl-relativetimeformat": "10.0.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@mdi/js": "6.5.95",
    "@mdi/react": "1.5.0",
    "axios": "0.26.0",
    "caniuse-lite": "1.0.30001312",
    "core-js": "3.21.1",
    "cross-env": "7.0.3",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.6",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "reselect": "4.1.5",
    "uuid": "8.3.2"
  },
