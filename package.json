{
  "name": "if.portfolio",
  "version": "1.2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furdzik/if.portfolio.git"
  },
  "scripts": {
    "start": "npm run lang && npm run dev",
    "build": "cross-env NODE_ENV=production npx webpack --progress --env production",
    "build:prod": "node ./scripts/create-env.js && npm run build",
    "lint": "npm run eslint && npm run stylelint",
    "lang": "extract-messages -l=en -d=en -o src/lang/generated --flat \"src/**/**/**/*.messages.js\"",
    "find-duplicates": "jscpd ./src/",
    "stylelint": "npx stylelint \"**/*.styles.{js,jsx}\"",
    "eslint": "eslint src/ --ext .jsx,.js",
    "dev": "npx webpack-dev-server --history-api-fallback --open --env development",
    "outdated": "npm outdated",
    "audit": "npm audit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.1.2",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@formatjs/intl-pluralrules": "4.3.3",
    "@formatjs/intl-relativetimeformat": "10.0.1",
    "@material-ui/core": "4.12.4",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@mdi/js": "6.6.96",
    "@mdi/react": "1.6.0",
    "@sentry/react": "7.0.0",
    "@sentry/tracing": "7.0.0",
    "axios": "0.27.2",
    "caniuse-lite": "1.0.30001341",
    "core-js": "3.23.1",
    "cross-env": "7.0.3",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-intl": "5.25.1",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "reselect": "4.1.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.5",
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-transform-runtime": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/runtime": "7.18.3",
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@emotion/babel-plugin": "11.9.2",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "babel-loader": "8.2.5",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-react-intl-auto": "3.3.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "commitlint": "17.0.2",
    "commitlint-config-kamisama": "0.3.1",
    "copy-webpack-plugin": "10.2.4",
    "dotenv": "14.2.0",
    "editorconfig": "0.15.3",
    "eslint": "8.18.0",
    "eslint-config-kamisama": "0.3.1",
    "eslint-import-resolver-babel-module": "5.3.1",
    "extract-react-intl-messages": "4.1.1",
    "file-loader": "6.2.0",
    "full-icu": "1.4.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jscpd": "3.4.5",
    "postcss": "8.4.13",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "18.1.0",
    "redux-devtools-extension": "2.13.9",
    "stylelint": "14.9.1",
    "stylelint-config-kamisama": "0.4.1",
    "stylelint-processor-styled-components": "1.10.0",
    "webpack": "5.73.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cdn-plugin": "3.3.1",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2",
    "webpack-merge": "5.8.0"
  }
}
